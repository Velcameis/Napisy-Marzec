#include <iostream>

using namespace std;

int main()
{
    string napis;
    int licznik = 0;
    getline (cin, napis);
    for (int i=0 ; i<napis.size() ; i++)
    {
        if(napis[i]=='A')licznik++;

    }
    cout << licznik;
}

